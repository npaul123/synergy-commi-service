{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*",
  "JsonConfiguration": {
    "JsonFolderPath": "/home/ec2-user/nfsbackup/notification/"
  },

  "Kestrel": {
    "Limits": {
      "MaxConcurrentConnections": 100,
      "MaxConcurrentUpgradedConnections": 100
    },
    "DisableStringReuse": true,
    "EndPoints": {
      "Http": {
        "Url": "http://0.0.0.0:16310"
      }
    }
  }
}

